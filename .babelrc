{
  "presets": [
    [
      "env",
      {
        "useBuiltins": true,
        "modules": false
      }
    ],
    "react"
  ],
  "plugins": [
    "transform-class-properties",
    "transform-object-rest-spread",
    [
      "transform-runtime",
      {
        "regenerator": true
      }
    ]
  ],
  "env": {
    "test": {
      "presets": ["env", "react"],
      "plugins": ["transform-class-properties", "transform-object-rest-spread"]
    }
  }
}
